{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn"
  }
}
